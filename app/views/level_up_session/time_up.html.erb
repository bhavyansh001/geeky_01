<div class="container mt-5">
 <div class="text-center">
    <h1 class="mb-4">Your time is up!</h1>
    <p>Correct answers: <%= @corrects.count %></p>
    <%= render 'table', title: "Questions(corrects)", type: @corrects if @corrects.count != 0 %>
    <br>
    <p>Wrong answers: <%= @incorrects.count %></p>
    <%= render 'table', title: "Questions(incorrects)", type: @incorrects if @incorrects.count != 0 %>
    <br><br>
    <div class="d-flex justify-content-center">
      <%= link_to 'Play again', level_up_path, class: "btn btn-primary btn-spaced mr-2", autofocus: true %>
      <%= link_to 'Back to Practice area', practice_path, class: "btn btn-secondary btn-spaced" %>
    </div>
 </div>
</div>